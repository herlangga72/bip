{
    "scripts": {
        "dev": "routify -c dev:vite",
        "build": "svelte-check && routify -b && vite build && cd ../website/ && python html_templater.py",
        "dev:vite": "vite --host --port 8001",
        "serve": "vite preview",
        "validate": "svelte-check",
        "format": "prettier --write . && prettier --ignore-path .prettierignore --check --plugin-search-dir=. ."
    },
    "devDependencies": {
        "@roxi/routify": "^2.18.2",
        "@sveltejs/vite-plugin-svelte": "^1.0.0-next.11",
        "carbon-components-svelte": "^0.33.0",
        "carbon-icons-svelte": "^10.33.0",
        "carbon-preprocess-svelte": "^0.3.0",
        "clipboard-copy": "^4.0.1",
        "html-minifier": "^4.0.0",
        "prettier": "2.4.1",
        "prettier-plugin-svelte": "^2.4.0",
        "svelte": "^3.38.2",
        "svelte-check": "^2.1.0",
        "vite": "^2.3.8"
    },
    "dependencies": {
        "vite-plugin-windicss": "^1.4.9",
        "windicss": "^3.1.9"
    }
}
